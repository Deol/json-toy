  "description": "Json-treeify: Get tree string(├└│─) via json, support browser|node, browser none dependencies!",
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.0",
    "clipboardy": "^1.2.3",
    "cross-env": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-common-shake": "^2.0.1",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "rewire": "^2.5.2",
    "should": "^11.1.1"
  },
  "_doPackNeedDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.0",
    "cross-env": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-common-shake": "^2.0.1"
  },
  "main": "index.js",
  "bin": {
    "j-tree-str": "bin/j-tree-str.js",
    "jts": "bin/j-tree-str.js",
    "jtls": "bin/jtls.js"
  },
  "scripts": {
    "test": "npm run testWithJsCover",
    "testWithoutJsCover": "mocha --harmony",
    "testWithJsCover": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --harmony",
    "packSrc": "cross-env WEBPACK_ENV=dev    webpack --progress --colors --display-error-details",
    "packMin": "cross-env WEBPACK_ENV=build  webpack --progress --colors --display-error-details",
    "pack": "run-s  packSrc  packMin",
    "testone": "mocha --harmony ./test/cli/walk-dir.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Froguard/json-toy.git"
  },
  "engines": {
    "node": ">= 4.0",
    "npm": ">= 2"
  },
  "keywords": [
    "treeify",
    "tree string",
    "directory tree",
    "json tree",
    "object tree",
    "circular check",
    "travel safe",
    "keyPath"
  ],
